---
import { Image } from 'astro:assets';

interface Props {
    size: any;
    image?: any;
    alt: string;
    url?: string;
    target?: string;
    rel?: string;
}

const { size, image, url, alt, target = "_self", rel } = Astro.props;
---
<a href={url} target={target} rel={rel}>
    <Image src={image} alt={alt} height={size} />
</a>